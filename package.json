{
  "name": "pos-web-app",
  "version": "1.0.0",
  "title": "EnRich Pos WebApp",
  "main": "app/background.js",
  "author": "Pos Mango",
  "description": "Pos Mango App",
  "build": {
    "appId": "Pos Mango",
    "asar": true,
    "files": [
      "main",
      "./app"
    ],
    "productName": "PosMangoApp",
    "win": {
      "icon": "public/favicon.ico"
    }
  },
  "keywords": [
    "react",
    "nextjs",
    "nextjs-starter",
    "nextjs-template",
    "typescript",
    "redux",
    "redux-saga",
    "ant-design",
    "styled-components",
    "jest",
    "enzyme"
  ],
  "license": "MIT",
  "scripts": {
    "copyfile": "xcopy \".\\pages\\loading-app.html\" \".\\app\" /Y /D",
    "start": "next start",
    "dev": "next ",
    "starte": "electron .",
    "deve": "nextron ",
    "build": "next build",
    "builde": "nextron build",
    "commit": "git-cz",
    "test": "jest",
    "coverage": "yarn jest --coverage",
    "lint": "yarn lint:eslint && yarn lint:css",
    "lint:eslint": "eslint --ext js,jsx,ts,tsx .",
    "lint:css": "stylelint **/*.{ts,tsx}",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "tsc:client": "tsc --noEmit -p tsconfig.json",
    "analyze": "cross-env ANALYZE=true next build",
    "bump-version": "standard-version --skip.commit --skip.tag"
  },
  "lint-staged": {
    "*.{tsx,ts,js,jsx}": [
      "stylelint",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  },
  "dependencies": {
    "create-react-class": "^15.7.0",
